	<xs:complexType name="DestinationCategoryType">
		<xs:choice>
			<xs:element name="Country" type="CountryCodeISO3166" minOccurs="0">
				<xs:annotation>
					<xs:documentation>2 character country code as defined in ISO3166</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Airport" type="AirportCodeIATA" minOccurs="0">
				<xs:annotation>
					<xs:documentation>3 character aiport code (IATA)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Region" minOccurs="0" type="RegionType"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="RegionType">
		<xs:simpleContent>
			<xs:extension base="RegionEnumType">
				<xs:attribute name="IncludeExtendedRegions" type="xs:boolean" default="true">
					<xs:annotation>
						<xs:documentation>flag indicates that it will be searched in extended regions. If set to true (default) requests
							with 'w' will
							also return product for wouk.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="RegionEnumType">
		<xs:annotation>
			<xs:documentation>Germany, Europe, World, Word without USA and Canada (only useful if you distinguish between w and wouk,
				otherwise use only w).
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="d"/>
			<xs:enumeration value="e"/>
			<xs:enumeration value="w"/>
			<xs:enumeration value="wouk"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CountryCodeISO3166">
		<xs:annotation>
			<xs:documentation xml:lang="en">2 character country code as defined in ISO3166.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[a-zA-Z]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AirportCodeIATA">
		<xs:annotation>
			<xs:documentation xml:lang="en">3 character aiport code IATA.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[a-zA-Z]{3}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ContingentType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Contingent maximum length</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:long">
			<xs:minInclusive value="1"/>
			<xs:pattern value="\d{1,9}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="DescriptionType">
		<xs:sequence>
			<xs:element name="Title" type="xs:string"/>
			<xs:choice>
				<xs:element name="Description" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>description.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="URL" minOccurs="0" maxOccurs="unbounded" type="TypedUrlType">
					<xs:annotation>
						<xs:documentation>Link to description.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TravelerAllocationType">
		<xs:attribute name="ID" type="xs:positiveInteger" use="required">
			<xs:annotation>
				<xs:documentation>Unique identifier to specify the allocation of one traveler for one tariff.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TravelerIDRef" type="xs:positiveInteger" use="required">
			<xs:annotation>
				<xs:documentation>Reference to SearchTraveler ID or CoveredTraveler ID - to specify the traveler this tariff is allocated for.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CoInsured" type="xs:boolean" default="false">
			<xs:annotation>
				<xs:documentation>flag indicates if the traveler is co-insured (default false)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="RequestServicesType">
		<xs:sequence>
			<xs:element name="Service" type="RequestServiceType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>a service description: contains reference to selected tariff or complete insurance plan AND  the traveler allocation</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<!-- BE 21.10.05-->
	</xs:complexType>
	<xs:complexType name="RequestServiceType">
		<xs:sequence>
			<xs:element name="QuotedTariff" type="QuoteTariffType"/>
			<xs:element name="TravelerAllocations" type="TravelerAllocationsType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Container to allocate the current tariff for the specified travelers.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DynamicPremiumOfferId" type="StringLength0to64" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Dynamic Premium Offer Id, if available.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ID" type="xs:positiveInteger" use="required">
			<xs:annotation>
				<xs:documentation>Unique identifier (one for each service request) to identify the service and additional sorting information</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Quantity" type="Numeric1to9999" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Number of units purchased.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TravelerAllocationsType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Container to allocate the current tariff for the specified travelers.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TravelerAllocation" type="TravelerAllocationType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Allocate the current tariff for one traveler and specify if the traveler is co-insuranced.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="QuoteTariffType">
		<xs:annotation>
			<xs:documentation>Tarif</xs:documentation>
		</xs:annotation>
			<xs:choice>
				<xs:element name="TariffCode" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>tariff key</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PlanCodeRef" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>reference to ths PlanID of a AvailablePlan</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TariffDetails" minOccurs="0" type="TariffDetailsType">
					<xs:annotation>
						<xs:documentation xml:lang="en">Details of requested tariff.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
	</xs:complexType>
	<xs:complexType name="EmailPolicyType">
		<xs:sequence>
			<xs:element name="AdditionalEmail" type="xs:string" minOccurs="0" maxOccurs="2">
				<xs:annotation>
					<xs:documentation>If any additional email about the policy content might be send (e.g. to travel agent) - the email address has to be set here (don't set the email of the InsuranceCustomer)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ReservationType">
		<xs:attribute name="Requested" type="xs:boolean" default="true">
			<xs:annotation>
				<xs:documentation xml:lang="en">Flag that indicated that this booking is meant to be a reservation request only.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="InsuredPersonSearchCritType">
		<xs:complexContent>
			<xs:extension base="PersonNameType">
				<xs:attribute name="ExactMatch" type="xs:boolean" use="optional" default="false">
					<xs:annotation>
						<xs:documentation xml:lang="en">It TRUE the returned names have to match exactly the submitted name.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CoveredTripSearchCritType">
		<xs:sequence>
			<xs:element name="StartDate" type="PeriodType" minOccurs="0"/>
			<xs:element name="EndDate" type="PeriodType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PeriodType">
		<xs:sequence>
			<xs:element name="FromDate" type="xs:dateTime"/>
			<xs:choice>
				<xs:element name="ToDate" type="xs:dateTime" minOccurs="0"/>
				<xs:element name="Duration" type="DurationType" minOccurs="0"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PolicyDetailSearchCritType">
		<xs:sequence>
			<xs:choice minOccurs="0">
				<xs:element name="PolicyNumber" type="PolicyRangeType">
					<xs:annotation>
						<xs:documentation xml:lang="en">Unique policy number assigned by the insurance company upon issuing the policy.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ExternalPolicyNumber" type="StringLength1to32">
					<xs:annotation>
						<xs:documentation xml:lang="en">The unique number of an associated policy in an external system.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OfferIdList" type="OfferIdListTyp">
					<xs:annotation>
						<xs:documentation xml:lang="en">List of offer IDs resulting in booking requests.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="OrderDate" type="PeriodType" minOccurs="0"/>
			<xs:element name="CancellationDate" type="PeriodType" minOccurs="0"/>
			<xs:element name="TravelAgent" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Expedient</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--xs:element name="CancellationDate" type="PeriodType" minOccurs="0"/-->
		</xs:sequence>
		<xs:attribute name="Status" type="StatusType" use="optional">
			<xs:annotation>
				<xs:documentation>Policy state: CONFIRMED, CANCELLED, PARTIALLY_CANCELLED, RESERVED, ANNULLED</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="OfferIdListTyp">
		<xs:sequence>
			<xs:element name="OfferId" type="StringLength1to255" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">ID referring the corresponding offer.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="ReservationExpirationAction">
		<xs:annotation>
			<xs:documentation>The action taken by the system when a booking reservation expires</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="COMMIT"/>
			<xs:enumeration value="ROLLBACK"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StatusType">
		<xs:annotation>
			<xs:documentation>Policy state: CONFIRMED, CANCELLED, PARTIALLY_CANCELLED, RESERVED, ANNULLED</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="CONFIRMED"/>
			<xs:enumeration value="CANCELLED"/>
			<xs:enumeration value="PARTIALLY_CANCELLED"/>
			<xs:enumeration value="RESERVED"/>
			<xs:enumeration value="ANNULLED"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CancelReasonType">
		<xs:annotation>
			<xs:documentation>Cancellation reasons: Customer oder Supplier cancellation or supplier insolvency. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="CUSTOMER_CANCELLATION"/>
			<xs:enumeration value="OPERATOR_CANCELLATION"/>
			<xs:enumeration value="INSOLVENCY"/>
			<xs:enumeration value="GOODWILL"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ReservationDetailsType">
		<xs:attribute name="ExpirationDate" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">The date and time when the reservation expires.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ExpirationAction" type="ReservationExpirationAction" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">The action taken by the system when the reservation expires</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!-- BE 27.10.05 -->
	<xs:complexType name="PolicyDetailType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Contains such specific, policy-identifying details such as policy number, reference number, effective and expiration dates.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PolicyNumber" type="UniqueID_Type">
				<xs:annotation>
					<xs:documentation xml:lang="en">Unique policy number assigned by the insurance company upon issuing the policy. "set type = 18 for insurance, set policy number as ID - other not in use"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExternalPolicyNumber" type="StringLength1to32" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">The unique number of an associated policy in an external system. Such a number can optionally be specified when creating a booking.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RefNumber" type="UniqueID_Type" minOccurs="0" maxOccurs="5">
				<xs:annotation>
					<xs:documentation xml:lang="en">"Not in use" Internal vendor reference number for policy.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PlanRestrictions" type="PlanRestrictionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Container for any restrictions to coverage of the insurance plan.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PolicyDetailURL" type="URL_Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Specifies the location of a file containing policy coverage details.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Status" type="StatusType">
				<xs:annotation>
					<xs:documentation>Policy state: CONFIRMED, CANCELLED, RESERVED, ANNULLED</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReservationDetails" type="ReservationDetailsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>additional details about a booking with status RESERVED</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PolicyPeriod" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>duration of the policy: JV= Jahresversicherung | K= Kurztrip </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ParentPolicyNumber" type="xs:positiveInteger" minOccurs="0">
				<xs:annotation>
					<xs:documentation>parent policy was canceled - this is the new policy</xs:documentation>
				</xs:annotation>
				<!-- BE 12.10.05 -->
			</xs:element>
			<xs:element name="ChildPolicyNumber" type="xs:positiveInteger" minOccurs="0">
				<xs:annotation>
					<xs:documentation>this policy was canceled - the child policy is the new one</xs:documentation>
				</xs:annotation>
				<!-- BE 12.10.05 -->
			</xs:element>
			<xs:element name="AdditionalPolicyInfo" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Any additional booking information, e.g. order number of used bookingtool.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OfferId" type="StringLength1to255" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">ID referring the corresponding offer.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="OrderDate" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">The date the insurance booking occured on.</xs:documentation>
			</xs:annotation>
			<!-- BE 12.10.05 (set as required)-->
		</xs:attribute>
		<xs:attribute name="OrderTravelAgent" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Expedient booking the insurance contract. </xs:documentation>
			</xs:annotation>
			<!-- BE 12.10.05 -->
		</xs:attribute>
		<xs:attribute name="CancelDate" type="xs:dateTime" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The date the insurance contract was canceled.</xs:documentation>
			</xs:annotation>
			<!-- BE 12.10.05 -->
		</xs:attribute>
		<xs:attribute name="CancelTravelAgent" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Expedient canceled the insurance contract.</xs:documentation>
			</xs:annotation>
			<!-- BE 12.10.05 -->
		</xs:attribute>
		<xs:attribute name="WaiverOfAdvice" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">True if the customer expressly waived his right to advice with regard to this particular booking.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="EffectiveExpireOptionalDateGroup"/>
	</xs:complexType>
	<xs:complexType name="CancelTravelerType">
		<xs:attribute name="TravelerIDRef" type="xs:positiveInteger" use="required"/>
	</xs:complexType>
	<xs:complexType name="CancelServiceType">
		<xs:attribute name="ServiceIDRef" type="xs:positiveInteger" use="required"/>
	</xs:complexType>
	<xs:complexType name="CancelAllocationType">
		<xs:attribute name="AllocationIDRef" type="xs:positiveInteger" use="required"/>
	</xs:complexType>
	<xs:complexType name="CoveredTravelersType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Container for information about the travelers requesting coverage by the insurance plan</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CoveredTraveler" type="CoveredTravelerType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Contains identifying information, coverage requirements and journey information for each individual requesting coverage by the insurance plan.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PolicyType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Contains a all policy details of a single insurance plan.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PolicyDetail" type="PolicyDetailType">
				<xs:annotation>
					<xs:documentation>"attributes EffectiveDate and ExpireDate not in use"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CoveredTravelers" type="CoveredTravelersType" minOccurs="0"/>
			<xs:element name="InsCoverageDetail" type="InsCoverageDetailType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Provides vendor with information about the traveler(s)' requirements for insurance coverage.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PlanCost" type="PlanCostType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Contains information about the total price of the insurance plan.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Services" type="InsuranceServicesType">
				<xs:annotation>
					<xs:documentation xml:lang="de">insurance services</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExtendedServices" type="ExtendedInsuranceServicesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">extended insurance services</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Contact" type="ContactPersonType" minOccurs="0" maxOccurs="5">
				<xs:annotation>
					<xs:documentation xml:lang="en">"Not in use" Defines contact information for an individual or company (for example, the provider's callcenter number or claims hotline).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="InsuranceCustomer" type="InsuranceCustomerType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>policy holder information, only in use:
 0..2 Telephone, 0..1 Email, 1 Address, 0..1 PaymentForm. 

For Address in use: 
1 StreetNmbr, 1 CityName, 1 PostalCode, 1 CountryName and 0..1 CompanyName, 0..1 AddressLine    

For PaymentForm only in use: 
PaymentCard or BankAcct

For PaymentCard in use:
1 CardCode, 1 CardNumber, 1 ExpireDate, 1 CardHolderName, 0..1 CardHoldterRPH (Verification code)

For BankAcct in use:
1 BankID, 1 BankAcctNumber, 0..1 BankAcctName, 0..1 BankName</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SupplementaryAttribs" type="SupplementaryAttribsType" minOccurs="0" maxOccurs="1"/>
			<!-- BE 12.10.05 -->
		</xs:sequence>
		<xs:attributeGroup ref="PlanID_Group"/>
		<xs:attribute name="RPH" type="RPH_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Maps plan booking response to single plan book request in the OTA_InsuranceBookRQ message</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="InsuranceCompany" type="StringLength2to4" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">Insurance offering this plan.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<!--BE 28.10.2005-->
	</xs:complexType>
	<xs:complexType name="PolicyRangeType">
		<xs:sequence>
			<xs:element name="FromPolicyNumber" type="xs:positiveInteger">
				<xs:annotation>
					<xs:documentation xml:lang="en">To submit only one policy number (no range).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ToPolicyNumber" type="xs:positiveInteger" minOccurs="0"/>
		</xs:sequence>
		<!--BE 28.10.2005-->
	</xs:complexType>
</xs:schema>
